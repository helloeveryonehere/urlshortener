{
  "parser": "babel-eslint",
  "extends": [
    "airbnb"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "codeFrame": false
  },
  "plugins": [
    "import"
  ],
  "env": {
    "node": true,
    "mocha": true
  },
  "globals": {
    "expect": true,
    "fetch": "readable"
  },
  "rules": {
    "flowtype/no-types-missing-file-annotation": 0,
    "prefer-destructuring": ["error", {"object": true, "array": false}],
    "no-console": [
      "error"
    ],
    "max-len": [
      "error",
      100
    ],
    "indent": [
      "error",
      2
    ],
    "class-methods-use-this": [
      "warn"
    ],
    "prefer-default-export": "off",
    "import/prefer-default-export": "off",
    "one-var": "off",
    "no-underscore-dangle": ["error", {"allowAfterThis": true} ]
  }
}